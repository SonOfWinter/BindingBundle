{
    "name": "sonofwinter/binding-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "This Bundle provides a binding from array to Entity with Symfony",
    "keywords": [
        "symfony",
        "php",
        "bundle",
        "binding"
    ],
    "authors": [
        {
            "name": "Thomas Leduc",
            "email": "thomaslmoi15@hotmail.fr",
            "role": "Developer"
        }
    ],
    "autoload":{
        "psr-4":{
            "SOW\\BindingBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/yaml": "^7.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.15",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/phpunit-bridge": "^7.0"
    }
}
